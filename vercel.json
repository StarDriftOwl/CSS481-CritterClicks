{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "routes": [
      { "handle": "filesystem" },
      { "src": "/", "dest": "/index.html" },
      { "src": "/pet", "dest": "/pet.html" },
      { "src": "/gallery", "dest": "/gallery.html" },
      { "src": "/rules", "dest": "/rules.html" },
      { "src": "/credits", "dest": "/credits.html" },
      { "src": "/settings", "dest": "/settings.html" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }